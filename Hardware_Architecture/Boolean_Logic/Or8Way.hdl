/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=W1);
    Or(a=W1 , b=in[2] , out=W2 );
    Or(a=W2 , b=in[3] , out=W3 );
    Or(a=W3 , b=in[4] , out=W4 );
    Or(a=W4 , b=in[5] , out=W5 );
    Or(a=W5 , b=in[6] , out=W6 );
    Or(a=W6 , b=in[7] , out=out );
}
